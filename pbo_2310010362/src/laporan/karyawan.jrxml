        <band height="20">
            <textField><reportElement x="0" y="0" width="80" height="20"/><textFieldExpression><![CDATA[$F{ID_Karyawan}]]></textFieldExpression></textField>
            <textField><reportElement x="80" y="0" width="120" height="20"/><textFieldExpression><![CDATA[$F{Nama_Karyawan}]]></textFieldExpression></textField>
